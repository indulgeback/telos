# Agent Service 配置

# 服务配置
PORT=8895
SERVICE_NAME=agent-service
REGISTRY_URL=http://localhost:8891

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=telos
DB_PASSWORD=telos_password
DB_NAME=telos

# Redis 配置
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# LLM 模型配置
# 支持的提供商: openai, deepseek, ark
LLM_PROVIDER=deepseek
LLM_API_KEY=your_api_key_here
LLM_MODEL=deepseek-chat
# 可选模型: deepseek-chat, deepseek-reasoner
LLM_BASE_URL=

# 豆包 Ark 配置 (使用 LLM_PROVIDER=ark 时配置)
LLM_ARK_API_KEY=
LLM_ARK_ENDPOINT=

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=color
LOG_OUTPUT=stdout
